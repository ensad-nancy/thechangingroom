{{debug}}
{{#each posts}}
    <div class="container abstract">
      <h1>{{{caption}}}</h1>
      <h1>{{title}}</h1>
        <div class="tags">
          {{#each tags}}
            <a href="#tags">#{{this}}</a>
          {{/each}}
        </div>

      </div>

    <div class="col-sm-12">
      {{{body}}}

          {{#each photos}}
                <img src="{{original_size.url}}" class="img-responsive" >
              {{/each}}

          {{{player.1.embed_code}}}
    </div>
{{/each}}
